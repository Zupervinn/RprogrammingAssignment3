## part 1
## lowest 30 death rate


library(dplyr)

best <- function(state, outcome){
        read_file<- read.csv("outcome-of-care-measures.csv", colClasses = "character", na.string ="Not Availablee", stringsAsFactors = FALSE )
        x <- filter(read_file, State==state)
        y <- select(x, contains(outcome))
        return(x)
}
}

}
#sort by col
function (data, col1, col2){
        orderdata <- data[order(data[,col1],data[,col2]),]
        return(orderdata)
}
#sort by State
function (data, country, column){
        countrydata <- data [grep(country,data$State),]
        orderdata <- countrydata[order(countrydata[,column]),]
        return (orderdata)
}
